{
   "version": 2,
   "builds": [
     {
       "src": "/package.json",
       "use": "@vercel/static-build",
       "config": {
         "distDir": "dist"
       }
     }
   ],
   "rewrites": [
     {
       "source": "/(css|js|images|icons)/(.*)",
       "destination": "/dist/$1/$2"
     },
     {
       "source": "/manifest.json",
       "destination": "//dist/manifest.json"
     },
     {
       "source": "/sw.js",
       "destination": "/dist/sw.js"
     },
     {
       "source": "/(.*)",
       "destination": "/dist/index.html"
     }
   ],
   "headers": [
     {
       "source": "/sw.js",
       "headers": [
         {
           "key": "Cache-Control",
           "value": "public, max-age=0, must-revalidate"
         },
         {
           "key": "Service-Worker-Allowed",
           "value": "/"
         }
       ]
     }
   ]
 }