{
   "version": 2,
   "builds": [
     {
       "src": "package.json",
       "use": "@vercel/static-build",
       "config": {
         "distDir": "dist"
       }
     }
   ],
   "rewrites": [
     {
       "source": "/manifest.json",
       "destination": "/dist/manifest.json"
     },
     {
       "source": "/sw.js",
       "destination": "/dist/sw.js"
     },
     {
       "source": "/(.*)",
       "destination": "/dist/index.html"
     }
   ]
 }